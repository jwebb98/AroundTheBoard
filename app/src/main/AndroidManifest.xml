<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.webb.aroundtheboard">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/menu_image"
        android:label="@string/newapp_name"
        android:roundIcon="@mipmap/menu_image_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".ATB_MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GameScreen"></activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>